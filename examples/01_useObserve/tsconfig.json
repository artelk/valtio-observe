{
  "compilerOptions": {
    "strict": true,
    "target": "es2021",
    "lib": ["es2021", "dom"],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "allowJs": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "jsx": "react-jsx"
  }
}
