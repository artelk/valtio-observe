{
  "compilerOptions": {
    "strict": true,
    "target": "es2021",
    "lib": ["es2021"],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "allowJs": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true
  }
}
